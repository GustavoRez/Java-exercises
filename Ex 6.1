class aula7{

  public static void main(String args[]){
  
   //Checking if the year is a leap year

	  int x = 2024;

	  if(x % 4 == 0 && x % 100 != 0){
	  System.out.println("O ano é bissexto");
	  } else if(x % 4 == 0 && x % 100 == 0 && x % 400 == 0){
	  System.out.println("O ano é bissexto");
	  } else{
	  System.out.println("Ano não bissexto");
    }
  }
}
